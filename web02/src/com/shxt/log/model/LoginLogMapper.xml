<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="com.shxt.log.dao.LoginLogDao">
        <insert id="add" parameterType="com.shxt.log.model.LoginLog">
               insert into login_log(log_id,user_id,user_name,log_date,ip)
               values
               (#{log_id},#{user_id},#{user_name},#{log_date},#{ip});
        </insert>
  </mapper>